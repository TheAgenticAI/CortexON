{
    "github": {
        "command": "npx",
        "args": [
            "-y",
            "@modelcontextprotocol/server-github"
        ],
        "env": {},
        "description": "GitHub MCP server for repository operations",
        "status": "enabled",
        "secret_key": "GITHUB_PERSONAL_ACCESS_TOKEN"
    },
    "google-maps": {
        "command": "npx",
        "args": [
            "-y",
            "@modelcontextprotocol/server-google-maps"
        ],
        "env": {
            "GOOGLE_MAPS_API_KEY": ""
        },
        "description": "Google Maps MCP server for geocoding, directions, and place search",
        "status": "enabled",
        "secret_key": "GOOGLE_MAPS_API_KEY"
    },
    "pinecone": {
      "command": "npx",
      "args": [
        "-y", "@pinecone-database/mcp"
      ],
      "env": {
        "PINECONE_API_KEY": ""
      },
      "description": "Pinecone MCP server for vector database operations, index management, and vector search",
      "status": "enabled",
      "secret_key": "PINECONE_API_KEY"
    }
}